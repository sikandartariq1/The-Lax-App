<div class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-11">
        <div class="card">
          <div class="header">
            <h4 class="title">Bank Account</h4>
            <p class="category">Details</p>
          </div>
          <div class="content">

            <%= form_tag old_salt_bank_account_path, method: :patch do |f| %>
              <%= render 'form' %>

              <%= submit_tag 'Update Details', class: 'btn btn-info btn-fill pull-right' %>
              <div class="clearfix"></div>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
